{
  "name": "midterm_template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "mdbreact": "^4.27.0",
    "react-native-background-audio": "^1.1.4",
    "react-typing-effect": "^2.0.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
